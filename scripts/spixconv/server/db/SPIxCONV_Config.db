# Autosave configuration
record(stringin, "$(P):SaveName"){
    field(PINI, "YES")
    field(DESC, "Autosave destination")
    field(VAL,  "autosave/save/$(P).sav")
}
record(stringin, "$(P):IPAddress"){
    field(DESC, "IP address expected for the BBB")
}
record(longin, "$(P):BoardAddress"){
    field(DESC, "SPI address")
}
