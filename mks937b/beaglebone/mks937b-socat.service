[Unit]
Description=Initialize the serial port bind.

[Service]
Restart=on-failure
RestartSec=5
Type=simple
ExecStart=/bin/bash /root/sirius-temporary-dev-repo/mks937b/beaglebone/init.sh
Environment=SOCAT_PORT=4161
Environment=SERVER_IP_ADDR=10.0.6.44
Environment=SERVER_MASK=255.255.255.0
StandardOutput=syslog
StandardError=syslog
 
[Install]
WantedBy=multi-user.target
