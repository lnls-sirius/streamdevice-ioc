
record(ai, "$(DEVICE):RelayRb-1") {
    field(DESC, "Read Setpoint for relay 1") 
    field(PINI, "YES") 
    field(DTYP, "stream")
    field(INP,"@mks937b.proto get_sp($(ADDRESS),1) $(PORT)")
}
record(ao, "$(DEVICE):RelaySp-1") {
    field(DESC, "Setpoint for relay 1") 
    field(DTYP, "stream")
    field(OUT,"@mks937b.proto set_sp($(ADDRESS),1) $(PORT)")
    field(FLNK, "$(DEVICE):RelayRb-1")
}

# Hysteresis
record(ai, "$(DEVICE):RelayHystRb-1") {
    field(DESC, "Read Setpoint Hysteresis for relay 1") 
    field(PINI, "YES") 
    field(DTYP, "stream")
    field(INP,"@mks937b.proto get_relay_hyst($(ADDRESS),1) $(PORT)")
}
