    field(DESC, "Setpoint for relay Hysteresis 1")
    field(DTYP, "stream")
    field(OUT,"@mks937b.proto set_relay_hyst($(ADDRESS),1) $(PORT)")
    field(FLNK, "$(DEVICE):RelayHystRb-1")
}
  
# Direction
record(mbbi, "$(DEVICE):RelayDirectionRb-1") {
  field(DESC, "Direction for the relay 1")
  field(DTYP, "stream")
  field(INP,  "@mks937b.proto get_relay_direction($(ADDRESS),1) $(PORT)") 
  field(PINI, "YES") 
  
  field(ZRVL, "0")
  field(ONVL, "1")
  field(ZRST, "ABOVE")
  field(ONST, "BELOW")
}
record(mbbo, "$(DEVICE):RelayDirectionSp-1"){
    field(DESC, "Set the relay 1 direction")
    field(DTYP, "stream")
    field(OUT, "@mks937b.proto set_relay_direction($(ADDRESS),1) $(PORT)")
     
    field(UNSV, "INVALID")
    field(IVOA, "Don't drive outputs")

    field(ZRVL, "0")
    field(ONVL, "1")
    field(ZRST, "ABOVE")
    field(ONST, "BELOW")

    field(FLNK, "$(DEVICE):RelayDirectionRb-1")
}
   
